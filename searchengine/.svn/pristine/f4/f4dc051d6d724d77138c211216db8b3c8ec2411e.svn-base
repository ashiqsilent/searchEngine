<%@ Page Title="" Language="C#" MasterPageFile="~/MasterPage.master" AutoEventWireup="true" CodeFile="addBusiness.aspx.cs" Inherits="addBusiness" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" Runat="Server">
    <link rel="stylesheet" type="text/css" href="/formStyle.css" />
    <script>
        $(document).keypress(function (e) {
            if (e.which == 13) {
                e.preventDefault();
                $('#ContentPlaceHolder1_button').click();
            }
        });
    </script>
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" Runat="Server">
        <div id="head" runat="server">
            <div id="wrap">
                <div id="login" class="animate form">
                    <h1 style="font-size: xx-large"> Register Your Business</h1>
                    <p>
                        <label for="businessname" data-icon="n"><strong>Business Name</strong></label>
                        <input id="businessname" name="businessname" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="category" data-icon="cat" ><strong>Category</strong></label>
                        <input id="category" name="category" type="text" runat="server"/> 
                    </p>
                    <p>
                        <label for="address" data-icon="a" ><strong>Address</strong></label>
                        <input id="address" name="address" type="text" runat="server"/> 
                    </p>
                    <p> 
                        <label for="province" data-icon="pr"><strong>Province</strong></label>
                        <input id="province" name="province" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="postalcode" data-icon="post"><strong>Postal Code</strong></label>
                        <input id="postalcode" name="postalcode" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="city" data-icon="c"><strong>City</strong></label>
                        <input id="city" name="city" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="country" data-icon="co"><strong>Country</strong></label>
                        <input id="country" name="country" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="phone" data-icon="p"><strong>Phone</strong></label>
                        <input id="phone" name="phone" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="email" data-icon="e"><strong>Email</strong></label>
                        <input id="email" name="email" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="website" data-icon="w"><strong>Website(Optional)</strong></label>
                        <input id="website" name="website" type="text" runat="server"/>
                    </p>
                    <p>
                        <label for="fax" data-icon="f"><strong>Fax(Optional)</strong></label>
                        <input id="fax" name="fax" type="text" runat="server"/>
                    </p>
                    <p class="signin button">
                        <asp:Button ID="button" runat="server" Text="Register" OnClick="button_Click"/>
					</p>
                </div>						
            </div>
        </div>
